{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Nest.js",
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "moduleResolution": "node",
    "sourceMap": true,
    "incremental": true,
    "allowJs": true
  }
}