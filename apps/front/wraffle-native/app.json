{
  "expo": {
    "name": "wraffle-native",
    "slug": "wraffle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "wraffle",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "googleServicesFile": "/Users/jangdonghyeon/Desktop/wraffle/apps/front/wraffle-native/firebase/GoogleService-Info.plist",
      "bundleIdentifier": "com.ww8007.wraffle",
      "supportsTablet": false
    },
    "android": {
      "googleServicesFile": "/Users/jangdonghyeon/Desktop/wraffle/apps/front/wraffle-native/firebase/google-services.json",
      "package": "com.ww8007.wraffle",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "@react-native-firebase/app"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2ee4d84c-352b-4e08-aacf-9885abf9b09a"
      }
    }
  }
}
