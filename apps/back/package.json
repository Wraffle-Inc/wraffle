{
  "name": "wraffle-api",
  "version": "0.0.1",
  "description": "wraffle api",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build front-api && nest build admin-api && nest build batch",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:front": "nest start front-api",
    "start:front:dev": "nest start front-api --watch",
    "start:admin": "nest start admin-api",
    "start:admin:dev": "nest start admin-api --watch",
    "start:batch": "nest start batch",
    "start:batch:dev": "nest start batch --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:schema:sync": "NODE_ENV=test ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:sync",
    "test": "pnpm run test:schema:sync && jest -i",
    "test:watch": "pnpm run test:schema:sync && jest -i --watch",
    "test:front:watch": "pnpm run test:schema:sync && jest -i --watch -- apps/front-api",
    "test:admin:watch": "pnpm run test:schema:sync && jest -i --watch -- apps/admin-api",
    "test:batch:watch": "pnpm run test:schema:sync && jest -i --watch -- apps/batch",
    "test:front:e2e": "pnpm run test:schema:sync && jest --config ./apps/front-api/test/jest-e2e.json",
    "test:admin:e2e": "pnpm run test:schema:sync && jest --config ./apps/admin-api/test/jest-e2e.json",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert",
    "schema:drop": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:sync",
    "seed": "ts-node -r tsconfig-paths/register ./db/seeds/seeder.ts",
    "seed:refresh": "ts-node -r tsconfig-paths/register ./db/seeds/seeder.ts --refresh"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^6.0.0",
    "@nestjs/typeorm": "~10.0.2",
    "@tirke/node-cache-manager-ioredis": "^3.6.0",
    "@types/pluralize": "^0.0.33",
    "aws-sdk": "^2.1147.0",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.2",
    "cache-manager": "^5.7.6",
    "change-case": "^5.4.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.0.0",
    "iconv-lite": "^0.6.3",
    "ioredis": "^5.4.1",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.1.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.35",
    "nanoid": "^5.0.7",
    "nest-winston": "^1.7.0",
    "nestjs-seeder": "^0.3.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.6.0",
    "pluralize": "^8.0.0",
    "qs": "^6.10.3",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7",
    "swagger-ui-express": "^5.0.1",
    "tslib": "^2.3.1",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-transactional": "^0.5.0",
    "winston": "^3.8.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.0",
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.1.2",
    "@nestjs/testing": "^10.3.10",
    "@types/bcryptjs": "^2.4.2",
    "@types/cache-manager": "^4.0.6",
    "@types/express": "^4.17.11",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.14.10",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/qs": "^6.9.7",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@wraffle/typescript-config": "workspace:*",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-simple-import-sort": "^9.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.1",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "apps/(.*)": "<rootDir>/apps/$1",
      "env.config": "<rootDir>/env.config.ts"
    },
    "roots": [
      "<rootDir>/apps/back/apps"
    ],
    "restoreMocks": true,
    "setupFilesAfterEnv": [
      "./apps/infrastructure/testing/jest.setup.js"
    ]
  },
  "lint-staged": {
    "**/*.{ts,tsx}": "eslint --cache --fix",
    "**/*.{ts,tsx,js,css,md}": "prettier --write",
    "**/*.js": "eslint --cache --fix",
    "**/*.{js,css,md}": "prettier --write"
  }
}
